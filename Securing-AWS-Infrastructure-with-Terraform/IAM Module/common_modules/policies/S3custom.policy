{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket",
                "s3:GetBucketLocation",
                "s3:ListAllMyBuckets"
            ],
            "Resource": [
                "arn:aws:s3:::cy7900-terraform-lock-dev",
                "arn:aws:s3:::cy7900-terraform-lock-dev/*"
            ]
        },

        {
            "Sid": "ListBuckets",
            "Effect": "Allow",
            "Action":[

                "s3:ListAllMyBuckets",
                "s3:*"
                
                

            ],
            "Resource":"*"
        }


    ]
}